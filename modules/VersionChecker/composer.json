{
    "name": "aldarien/adminer-version-checker",
    "description": "Check for latest adminer version",
    "type": "module",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "aldarien/config": "@dev",
        "aldarien/path": "@dev",
        "aldarien/file-loaders": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "kint-php/kint": "^3.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aldarien",
            "email": "aldarien85@gmail.com"
        }
    ],
    "autoload": {
      "psr-4": {
        "Adminer\\App\\": "app"
      }
    },
    "repositories": [
      {
        "type": "path",
        "url": "../Config"
      },
      {
        "type": "path",
        "url": "../Path"
      },
      {
        "type": "path",
        "url": "../FileLoaders"
      }
    ]
}
